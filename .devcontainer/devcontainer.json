{
	"name": "HWRS564a Course Materials",
	"image": "mcr.microsoft.com/devcontainers/python:3.11",
	"features": {
		"ghcr.io/va-h/devcontainers-features/uv:1": {}
	},
	"postCreateCommand": "uv sync && python -c \"from flopy.utils import get_modflow; get_modflow('./modflow')\"" ,
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-toolsai.jupyter"
			]
		}
	},
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}"
}
